<h1>Wishlist</h1>


<% products.forEach(product => { %>
    <div class="product">
        <img src="<%= product.image.url %>" alt="<%= product.name %>">        
        <h2><%= product.name %></h2>
        <p>Price: Rs <%= product.price %></p>
        <p><%= product.description %></p>
    </div>
<% }); %>
